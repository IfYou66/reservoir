<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szsk.reservoir.workflow.mapper.WorkFlowMappper">

    <update id="updateBusinessActiviti"  parameterType="java.util.Map">
        update ${tableName}
        <set>
            <trim prefixOverrides=",">
                <if test="actId != null">f_act_id = #{actId},</if>
                <if test="actPersion != null">f_act_persion = #{actPersion},</if>
                <if test="status != null">f_status = #{status},</if>
                <if test="actNode != null">f_act_node = #{actNode},</if>
            </trim>
        </set>
        where f_id = #{businessKey}
    </update>

</mapper>