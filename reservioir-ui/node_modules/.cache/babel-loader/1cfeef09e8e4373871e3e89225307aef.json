AwEA,GAxEA,EAwEA;AAAA;;AACA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,MAAA,GAAA,CAAA;AACA,UAAA,GAAA,GAAA,GAAA,CAAA,GAAA,IAAA,KAAA,GAAA;AACA,qCAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA;AACA;AACA;AACA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,cAAA,MAAA,GAAA;AACA,YAAA,EAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AADA,WAAA;AAGA,iCAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,SAAA,EADA,CAEA;AACA;AACA,WAJA;AAKA,SATA,MASA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,SAAA;AACA,SAhBA,CAiBA;AACA;AACA;;AACA,OApBA;AAqBA,KAlGA;;AAmGA;AACA,IAAA,YApGA,wBAoGA,GApGA,EAoGA;AAAA;;AACA,WAAA,KAAA;AACA,UAAA,GAAA,GAAA,GAAA,CAAA,GAAA,IAAA,KAAA,GAAA;AACA,qCAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,GAAA,EAAA;AAFA,SAAA,CAAA;;AAIA,YAAA,QAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,cAAA,MAAA,GAAA;AACA,YAAA,EAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AADA,WAAA;AAGA,iCAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,GAAA,GAAA,GAAA,CAAA,IAAA,CAAA,SAAA;AACA,WAHA;AAIA;;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,WAAA;AACA,OAjBA;AAkBA,KAzHA;;AA0HA;AACA,IAAA,UA3HA,wBA2HA;AAAA;;AACA,WAAA,KAAA,CAAA,MAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,MAAA,CAAA,IAAA,CAAA,GAAA,IAAA,IAAA,EAAA;AACA,8CAAA,MAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,UAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA,WANA,MAMA;AACA,2CAAA,MAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,UAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA;AACA;AACA,OAhBA;AAiBA,KA7IA;;AA8IA;AACA,IAAA,YA/IA,wBA+IA,GA/IA,EA+IA;AAAA;;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,GAAA,IAAA,KAAA,GAAA;AACA,WAAA,QAAA,CAAA,sBAAA,IAAA,GAAA,QAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,eAAA,+BAAA,IAAA,CAAA;AACA,OANA,EAMA,IANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,MAAA;AACA,OATA,EASA,KATA,CASA,YAAA,CAAA,CATA;AAUA,KA3JA;;AA4JA;AACA,IAAA,YA7JA,0BA6JA;AACA,WAAA,QAAA,CAAA,uBAAA,kCACA,KAAA,WADA;AAGA;AAjKA;AA9CA,C","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <el-form :model=\"queryParams\" ref=\"queryForm\" :inline=\"true\" v-show=\"showSearch\" label-width=\"68px\">\n      <el-form-item label=\"考评名称\" prop=\"fEvaluationName\" label-width=\"120px !important\">\n        <el-input\n          v-model=\"queryParams.fEvaluationName\"\n          placeholder=\"请输入考评名称\"\n          clearable\n          size=\"small\"\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n<!--      <el-form-item label=\"考评时间\" prop=\"fEvaluationTime\" label-width=\"120px !important\">\n        <el-date-picker clearable size=\"small\"\n          v-model=\"queryParams.fEvaluationTime\"\n          type=\"date\"\n          value-format=\"yyyy-MM-dd\"\n          placeholder=\"选择考评时间\">\n        </el-date-picker>\n      </el-form-item>-->\n      <el-form-item label=\"考评时间\">\n        <el-date-picker v-model=\"dateRange\" size=\"small\" style=\"width: 240px\" value-format=\"yyyy-MM-dd\" type=\"daterange\"\n                        range-separator=\"-\" start-placeholder=\"开始日期\" end-placeholder=\"结束日期\"></el-date-picker>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\n      </el-form-item>\n    </el-form>\n\n    <el-row :gutter=\"10\" class=\"mb8\">\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"primary\"\n          plain\n          icon=\"el-icon-plus\"\n          size=\"mini\"\n          @click=\"handleAdd\"\n          v-hasPermi=\"['run:evaluation:add']\"\n        >新增</el-button>\n      </el-col>\n      <!--<el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"danger\"\r\n          plain\r\n          icon=\"el-icon-delete\"\r\n          size=\"mini\"\r\n          :disabled=\"multiple\"\r\n          @click=\"handleDelete\"\r\n          v-hasPermi=\"['run:evaluation:remove']\"\n        >删除</el-button>-->\n      </el-col>\n\n      <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\n    </el-row>\n\n    <el-table v-loading=\"loading\" :data=\"evaluationList\" @selection-change=\"handleSelectionChange\">\n   <!--   <el-table-column type=\"selection\" width=\"55\" align=\"center\" />-->\n      <el-table-column label=\"考评名称\" align=\"center\" prop=\"fEvaluationName\" />\n      <el-table-column :show-overflow-tooltip=\"true\" label=\"考评概要\" align=\"center\" prop=\"fEvaluationOnline\" />\n      <el-table-column label=\"考评时间\" align=\"center\" prop=\"fEvaluationTime\" width=\"180\">\n        <template slot-scope=\"scope\">\n          <span>{{ parseTime(scope.row.fEvaluationTime, '{y}-{m}-{d}') }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"scope\">\n            <el-button\n              size=\"mini\"\n              type=\"text\"\n              icon=\"el-icon-query\"\n              @click=\"handleGetInfo(scope.row)\"\n              v-hasPermi=\"['run:evaluation:query']\"\n            >查看</el-button>\n          <!--<el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-edit\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-hasPermi=\"['run:evaluation:edit']\"\r\n          >修改</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-delete\"\r\n            @click=\"handleDelete(scope.row)\"\r\n            v-hasPermi=\"['run:evaluation:remove']\"\r\n          >删除</el-button>-->\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination\n      v-show=\"total>0\"\n      :total=\"total\"\n      :page.sync=\"queryParams.pageNum\"\n      :limit.sync=\"queryParams.pageSize\"\n      @pagination=\"getList\"\n    />\n\n    <!-- 添加或修改洪水调度考评 对话框 -->\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"800px\" append-to-body>\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"150px !important\">\n        <el-form-item label=\"考评名称\"  prop=\"fEvaluationName\">\n          <el-input v-model=\"form.fEvaluationName\" :disabled=\"read\" placeholder=\"请输入考评名称\" />\n        </el-form-item>\n         <el-form-item label=\"考评时间\"  prop=\"fEvaluationTime\">\n          <el-date-picker clearable size=\"small\"\n            v-model=\"form.fEvaluationTime\"\n            type=\"date\"\n            :disabled=\"read\"\n            value-format=\"yyyy-MM-dd\"\n            placeholder=\"选择考评时间\">\n          </el-date-picker>\n        </el-form-item>\n        <el-form-item label=\"考评概要\"  prop=\"fEvaluationOnline\">\n          <el-input v-model=\"form.fEvaluationOnline\" :disabled=\"read\" type=\"textarea\" maxlength=\"500\" show-word-limit  placeholder=\"请输入内容\" rows=\"12\"/>\n        </el-form-item>\n\n        <!-- <el-form-item label=\"调度考评文件ID\" prop=\"fEvaluationFileId\">\n          <el-input v-model=\"form.fEvaluationFileId\" placeholder=\"请输入调度考评文件ID\" />\n        </el-form-item> -->\n         <el-form-item label=\"考评文件\" prop=\"fEvaluationFileId\">\n\t        <el-upload class=\"upload-demo\" style=\"font-size:100\" :action=\"GLOBAL.httpUrl\" multiple\n\t                   :on-success=\"successResave\" ref=\"upload\" :headers=\"GLOBAL.headers()\" :limit=\"1\" :on-preview=\"handlePreview\"\n\t                   :show-file-list=\"true\" :auto-upload=\"true\" :disabled=\"active != 1\" :on-remove=\"remove\"\n\t                   :file-list=\"fileList\" :before-upload = \"beforeAvatarUpload\" >\n\t          <el-button :disabled=\"active != 1\" type=\"primary\">上传考评文件</el-button>\n\t        </el-upload>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n        <el-button @click=\"cancel\">取 消</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { listEvaluation, getEvaluation, delEvaluation, addEvaluation, updateEvaluation } from \"@/api/run/evaluation\";\n\nimport {getFileById} from \"@/api/file\";\n\nexport default {\n  name: \"Evaluation\",\n  data() {\n    return {\n      // 日期范围\n      dateRange: [],\n      // 遮罩层\n      loading: true,\n      // 选中数组\n      ids: [],\n      // 非单个禁用\n      single: true,\n      // 非多个禁用\n      multiple: true,\n      // 显示搜索条件\n      showSearch: true,\n      // 总条数\n      total: 0,\n      // 洪水调度考评 表格数据\n      evaluationList: [],\n      // 弹出层标题\n      title: \"\",\n      // 是否显示弹出层\n      open: false,\n      // 查询参数\n      queryParams: {\n        pageNum: 1,\n        pageSize: 10,\n        fEvaluationName: null,\n        fEvaluationTime: null,\n        fCreatePeople: null,\n        fCreateTime: null\n      },\n      fileList: [],\n      active: 1,\n      // 表单参数\n      form: {},\n      // 表单校验\n      rules: {\n      },\n      read: false\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    /** 查询洪水调度考评 列表 */\n    beforeAvatarUpload(file){\n    const isLt100M = file.size / 1024 / 1024 < 100;\n       if (!isLt100M) {\n              this.$message.error('上传文件大小不能超过 100MB!');\n              return false\n          }\n    },\n    getList() {\n      this.loading = true;\n      listEvaluation(this.addDateRange(this.queryParams, this.dateRange)).then(response => {\n        this.evaluationList = response.rows;\n        this.total = response.total;\n        this.loading = false;\n      });\n    },\n    // 上传成功事件\n    successResave(response, file, fileList) {\n      this.form.fEvaluationFileId = response.data.fId\n    },\n    remove(file, fileList) {\n      this.GLOBAL.commonFun(this.form.fEvaluationFileId)\n    },\n    handlePreview(file) {\n\t  // window.location.href = this.fileList[0].url;\r\n\t  window.open(this.fileList[0].url);\n\t},\n    // 取消按钮\n    cancel() {\n      this.open = false;\n      this.reset();\n    },\n    // 表单重置\n    reset() {\n      this.active = 1\n      this.read = false\n      this.fileList = []\n      this.form = {\n        fId: null,\n        fEvaluationName: null,\n        fEvaluationOnline: null,\n        fEvaluationTime: null,\n        fEvaluationFileId: null,\n        fCreatePeople: null,\n        fCreateTime: null\n      };\n      this.resetForm(\"form\");\n    },\n    /** 搜索按钮操作 */\n    handleQuery() {\n      this.queryParams.pageNum = 1;\n      this.getList();\n    },\n    /** 重置按钮操作 */\n    resetQuery() {\n      this.resetForm(\"queryForm\");\n      this.handleQuery();\n    },\n    // 多选框选中数据\n    handleSelectionChange(selection) {\n      this.ids = selection.map(item => item.fId)\n      this.single = selection.length!==1\n      this.multiple = !selection.length\n    },\n    /** 新增按钮操作 */\n    handleAdd() {\n      this.reset();\n      this.open = true;\n      this.title = \"添加洪水调度考评 \";\n    },\n    /** 查看按钮操作 */\n    handleGetInfo(row) {\n      // this.reset();\n      this.read = true\n      this.active = 2\n      const fId = row.fId || this.ids\n      getEvaluation(fId).then(response => {\n//        this.fileList = [{\r\n//          name: '暂无文件',\r\n//          url: null\r\n//        }]\n          if(response.data.fEvaluationFileId){\n            let params = {\n                  id:response.data.fEvaluationFileId\n              }\n            getFileById(params).then(res => {\n            \twindow.open(res.data.fFilePath);\n//            this.fileList[0].name = res.data.fOldFileName\r\n//            this.fileList[0].url = res.data.fFilePath\n            })\n          }else{\n          \t this.msgError(\"没有上传附件！\");\n          }\n//      this.form = response.data;\r\n//      this.open = true;\r\n//      this.title = \"洪水调度考评 \";\n      });\n    },\n    /** 修改按钮操作 */\n    handleUpdate(row) {\n      this.reset();\n      const fId = row.fId || this.ids\n      getEvaluation(fId).then(response => {\n        this.form = response.data;\n        this.fileList = [{\n            name: '暂无文件',\n            url: null\n          }]\n          if(response.data.fEvaluationFileId){\n            let params = {\n                  id:response.data.fEvaluationFileId\n              }\n            getFileById(params).then(res => {\n              this.fileList[0].name = res.data.fOldFileName\n              this.fileList[0].url = res.data.fFilePath\n            })\n          }\n        this.open = true;\n        this.title = \"修改洪水调度考评 \";\n      });\n    },\n    /** 提交按钮 */\n    submitForm() {\n      this.$refs[\"form\"].validate(valid => {\n        if (valid) {\n          if (this.form.fId != null) {\n            updateEvaluation(this.form).then(response => {\n              this.msgSuccess(\"修改成功\");\n              this.open = false;\n              this.getList();\n            });\n          } else {\n            addEvaluation(this.form).then(response => {\n              this.msgSuccess(\"新增成功\");\n              this.open = false;\n              this.getList();\n            });\n          }\n        }\n      });\n    },\n    /** 删除按钮操作 */\n    handleDelete(row) {\n      const fIds = row.fId || this.ids;\n      this.$confirm('是否确认删除洪水调度考评 编号为\"' + fIds + '\"的数据项?', \"警告\", {\n          confirmButtonText: \"确定\",\n          cancelButtonText: \"取消\",\n          type: \"warning\"\n        }).then(function() {\n          return delEvaluation(fIds);\n        }).then(() => {\n          this.getList();\n          this.msgSuccess(\"删除成功\");\n        }).catch(() => {});\n    },\n    /** 导出按钮操作 */\n    handleExport() {\n      this.download('run/evaluation/export', {\n        ...this.queryParams\n      }, `run_evaluation.xlsx`)\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/run/evaluation"}]}