:"handling.vue","sourceRoot":"src/views/safetyManagement/Risk","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <!-- 头部箭头 -->\n    <el-row class=\"from-top\">\n      <el-col :span=\"24\">\n        <div style=\"margin: 0 auto;display: flex;align-items: center;width: 100%;justify-content:center;\">\n          <div style=\"position: relative;\">\n            <img src=\"../../../assets/images/jin.png\"/>\n            <div  style=\"position: absolute;top: 10px;left: 100px;width: 50%;color: #FFFFFF;\">1.险情发生</div>\n          </div>\n          <div  style=\"position: relative;\">\n            <img :src=\"require(active>1? '../../../assets/images/jin.png':'../../../assets/images/jin1.png')\" />\n            <div  :style=\"active>1? 'position: absolute;top: 10px;left: 100px;width: 50%;color: #FFFFFF;':'position: absolute;top: 10px;left: 100px;width: 50%;color: #757575;'\">2.应急响应</div>\n            </div>\n          <div   style=\"position: relative;\">\n            <img :src=\"require(active>2? '../../../assets/images/jin.png':'../../../assets/images/jin1.png')\" />\n            <div  :style=\"active>2? 'position: absolute;top: 10px;left: 100px;width: 50%;color: #FFFFFF;':'position: absolute;top: 10px;left: 100px;width: 50%;color: #757575;'\">3.险情排除</div>\n            </div>\n            <div   style=\"position: relative;\">\n              <img :src=\"require(active==4? '../../../assets/images/jin.png':'../../../assets/images/jin1.png')\" />\n              <div  :style=\"active==4? 'position: absolute;top: 10px;left: 100px;width: 50%;color: #FFFFFF;':'position: absolute;top: 10px;left: 100px;width: 50%;color: #757575;'\">4.处置检查完成</div>\n              </div>\n        </div>\n      </el-col>\n    </el-row>\n\n    <!-- 表主体 -->\n    <div  class=\"from-div\">\n      <!-- 头部按钮 -->\n      <el-row>\n        <el-col :span=\"24\"\n        class=\"from-div-title\"\n        style=\"background-color: rgba(215, 215, 215, 1); text-align: right;\"\n         >\n        <div style=\"margin-right: 15px\">\n          <el-button type=\"primary\" size=\"small\" @click=\"save\">暂 存</el-button>\n          <el-button type=\"primary\" size=\"small\" @click=\"addsubmit\">提 交</el-button>\n        </div>\n        </el-col>\n      </el-row>\n\n      <!-- 表名 -->\n      <el-row>\n        <el-col :span=\"24\" class=\"from-div-title\" >\n          水库大坝险情处置一览表\n        </el-col>\n      </el-row>\n\n      <!-- 险情 第一行 -->\n      <el-row class=\"from-border-bottom\" >\n        <el-col :span=\"3\" class=\"from-col-left\" >  险情名称 </el-col>\n        <el-col :span=\"4\" class=\"from-border-right\" >\n          <el-input v-model=\"MRiskManagement.fRiskName\" placeholder=\"请输入内容\"></el-input>\n        </el-col>\n\n        <el-col :span=\"2\" class=\"from-col-left\" >  险情类别 </el-col>\n        <el-col :span=\"3\" style=\"line-height: 40px;border-right: #a4d5ff 1px solid;\">\n            <div>\n              <el-select v-model=\"MRiskManagement.fRiskType\" placeholder=\"请选择\">\n                <el-option v-for=\"item in audit\" :key=\"item.dictValue\" :label=\"item.dictLabel\" :value=\"item.dictValue\">\n                </el-option>\n              </el-select>\n            </div>\n        </el-col>\n\n        <el-col :span=\"2\" class=\"from-col-left\" > 险情编号 </el-col>\n        <el-col :span=\"2\" class=\"from-border-right\" >\n          <el-input v-model=\"MRiskManagement.fRiskNumber\" placeholder=\"请输入内容\"></el-input>\n        </el-col>\n\n        <el-col :span=\"3\" class=\"from-col-left\" > 险情发现时间 </el-col>\n        <el-col :span=\"4\" style=\"text-align: center ;line-height: 40px;\">\n          <el-date-picker v-model=\"MRiskManagement.fRiskFindDate\" type=\"date\"  placeholder=\"开始时间\" format=\"yyyy-MM-dd\">\n          </el-date-picker>\n        </el-col>\n\n      </el-row>\n\n      <!-- 险情 第二行 概述 -->\n      <el-row  class=\"from-border-bottom\" >\n        <el-col :span=\"3\" class=\"from-borderibht\" style=\"line-height: 250px;\"> 险情概述 </el-col>\n        <el-col :span=\"21\" >\n\n          <div style=\"font-size: 16px;font-weight: 700;padding: 10px;\"> 险情概述： </div>\n          <el-input maxlength=\"500\" show-word-limit  type=\"textarea\" :rows=\"5\" placeholder=\"请输入内容\" v-model=\"MRiskManagement.fRiskSummary\">\n          </el-input>\n\n          <div style=\"display:flex;justify-content:space-between;\">\n\n            <div style=\"width: 200px;margin-left: 20px;\">\n              <el-upload\n               :before-upload = \"beforeAvatarUpload\"\n                class=\"upload-demo\"\n                :headers=\"GLOBAL.headers()\"\n                :action=\"GLOBAL.httpUrl\"\n                :on-preview=\"handlePreview\"\n                :on-remove=\"handleRemove\"\n\n                multiple\n                :on-success=\"uploadedSuccess\"\n                :show-file-list=\"true\"\n                :auto-upload=\"true\"\n                :limit=\"1\"\n                :on-exceed=\"handleExceed\"\n                :file-list=\"fileList\">\n                <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n\t\t\t\t<div slot=\"tip\" class=\"el-upload__tip\" style=\"margin-top: 15px\">上传文件不得超过100M</div>\n              </el-upload>\n            </div>\n\n            <div style=\"font-size: 20px;margin-right: 10px;margin-top: -5px;\">\n              <div style=\"text-align: left\">\n                <div style=\"display:inline;margin-right: -15px\">记录人：\n                  <el-input v-model=\"MRiskManagement.fRecordPeople\" placeholder=\"请输入内容\"\n                    style=\"width:220px;border: 1px solid #DCDFE6;margin-left: -6px\"></el-input>\n                </div>\n\n                <div class=\"block\" style=\"transform:translateY(10%)\">\n                  <span class=\"demonstration\" v-html=\"'时&ensp;&ensp;间：'\"/>\n                  <el-date-picker\n                    type=\"date\" style=\"border: 1px solid #DCDFE6;\"\n                    v-model=\"MRiskManagement.fRecordDate\"\n                    placeholder=\"选择日期\"\n                    format=\"yyyy-MM-dd\"\n                    value-format=\"yyyy-MM-dd\">\n                  </el-date-picker>\n                </div>\n              </div>\n            </div>\n          </div>\n\n        </el-col>\n      </el-row>\n\n      <!-- 险情 第三行 响应 -->\n      <el-row  class=\"from-border-bottom\" >\n        <el-col :span=\"3\" class=\"from-borderibht\" style=\"line-height: 250px;\"> 响应情况 </el-col>\n        <el-col :span=\"21\" >\n          <div style=\"font-size: 16px;font-weight: 700;padding: 10px;\"> 响应情况： </div>\n          <el-input maxlength=\"500\" show-word-limit  type=\"textarea\" :rows=\"5\" placeholder=\"请输入内容\" v-model=\"MRiskManagement.fResponseSituation\" :disabled=\"true\">\n          </el-input>\n\n          <div style=\"display:flex;justify-content:space-between;\">\n            <div style=\"width: 200px;margin-left: 20px;\">\n              <el-upload class=\"upload-demo\" action=\"https://jsonplaceholder.typicode.com/posts/\"\n                         :on-preview=\"handlePreview\" :on-remove=\"handleRemove\"  multiple\n                          :before-upload = \"beforeAvatarUpload\"\n                         :limit=\"3\"\n                         :on-exceed=\"handleExceed\" :file-list=\"fileList2\">\n                <el-button size=\"small\" type=\"primary\" :disabled=\"true\">点击上传</el-button>\r\n\t\t\t\t<div slot=\"tip\" class=\"el-upload__tip\" style=\"margin-top: 15px\">上传文件不得超过100M</div>\n              </el-upload>\n            </div>\n\n            <div style=\"font-size: 20px;margin-right: 10px;margin-top: -5px;\">\n              <div style=\"text-align: left\">\n                <div style=\"display:inline;margin-right: -15px\">响应人：\n                  <el-input v-model=\"MRiskManagement.fResponsePeople\" placeholder=\"请输入内容\" :disabled=\"true\"\n                    style=\"width:220px;border: 1px solid #DCDFE6;margin-left: -6px\"></el-input>\n                </div>\n                <div class=\"block\" style=\"transform:translateY(10%)\">\n                  <span class=\"demonstration\" v-html=\"'时&ensp;&ensp;间：'\"/>\n                  <el-date-picker\n                    type=\"dates\" style=\"border: 1px solid #DCDFE6;\"\n                    v-model=\"MRiskManagement.fResponseDate\"\n                    placeholder=\"选择日期\"\n                    format=\"yyyy 年 MM 月 dd 日\"\n                    value-format=\"yyyy-MM-dd HH:mm:ss\"\n                    :disabled=\"true\">\n                  </el-date-picker>\n                </div>\n              </div>\n            </div>\n          </div>\n\n        </el-col>\n      </el-row>\n\n      <!-- 险情 第四行 处置 -->\n      <el-row  class=\"from-border-bottom\" >\n        <el-col :span=\"3\" class=\"from-borderibht\" style=\"line-height: 250px;\"> 处置结果 </el-col>\n        <el-col :span=\"21\" >\n          <div style=\"font-size: 16px;font-weight: 700;padding: 10px;\">  处置结果： </div>\n          <el-input maxlength=\"500\" show-word-limit  type=\"textarea\" :rows=\"5\" placeholder=\"请输入内容\" v-model=\"MRiskManagement.fDisposalResults\" :disabled=\"true\">\n          </el-input>\n\n          <div style=\"display:flex;justify-content:space-between;\">\n            <div style=\"width: 200px;margin-left: 20px;\">\n              <el-upload class=\"upload-demo\" action=\"https://jsonplaceholder.typicode.com/posts/\"\n                         :on-preview=\"handlePreview\" :on-remove=\"handleRemove\"  multiple\n                          :before-upload = \"beforeAvatarUpload\"\n                         :limit=\"3\"\n                         :on-exceed=\"handleExceed\" :file-list=\"fileList\">\n                <el-button size=\"small\" type=\"primary\" :disabled=\"true\">点击上传</el-button>\r\n\t\t\t\t<div slot=\"tip\" class=\"el-upload__tip\" style=\"margin-top: 15px\">上传文件不得超过100M</div>\n              </el-upload>\n            </div>\n\n            <div style=\"font-size: 20px;margin-right: 10px;margin-top: -5px;\">\n              <div style=\"text-align: left\">\n                <div style=\"display:inline;margin-right: -15px\">处置人：\n                  <el-input v-model=\"MRiskManagement.fDisposalPeople\" placeholder=\"请输入内容\" :disabled=\"true\"\n                    style=\"width:220px;border: 1px solid #DCDFE6;margin-left: -6px\"></el-input>\n                </div>\n                <div class=\"block\" style=\"transform:translateY(10%)\">\n                  <span class=\"demonstration\" v-html=\"'时&ensp;&ensp;间：'\"/>\n                  <el-date-picker\n                    type=\"dates\" style=\"border: 1px solid #DCDFE6;\"\n                    v-model=\"MRiskManagement.fDisposalDate\"\n                    placeholder=\"选择日期\"\n                    format=\"yyyy 年 MM 月 dd 日\"\n                    value-format=\"yyyy-MM-dd HH:mm:ss\"\n                    :disabled=\"true\">\n                  </el-date-picker>\n                </div>\n              </div>\n            </div>\n          </div>\n\n        </el-col>\n      </el-row>\n\n      <!-- 险情 第五行 检查 -->\n      <el-row class=\"from-border-bottom\">\n        <el-col :span=\"3\" class=\"from-col\"> 处置检查 </el-col>\n        <el-col :span=\"21\" style=\"height: 50px;\">\n          <div style=\"font-size: 16px;font-weight: 700;padding: 10px;\"> 检查意见： </div>\n          <el-input maxlength=\"500\" show-word-limit  type=\"textarea\" placeholder=\"请输入内容\" :rows=\"2\" v-model=\"MRiskManagement.fInspectionOpinions\" :disabled=\"true\"> </el-input>\n\n          <div style=\"display: inline\">\n            <div style=\"margin-left: 10px;margin-top: 15px\">\n              <span style=\"font-size: 16px;font-weight: 700;display:inline;\"> 检查结果： </span>\n              <el-select v-model=\"MRiskManagement.fInspectionResults\" clearable placeholder=\"请选择\" style=\"border: 1px solid #DCDFE6;\" :disabled=\"true\">\n                <el-option v-for=\"item in audit\" :key=\"item.dictValue\" :label=\"item.dictLabel\" :value=\"item.dictValue\">\n                </el-option>\n              </el-select>\n            </div>\n\n            <div  style=\"text-align:right;margin-right: 15px;margin-top: -37px\">\n              <span >检查人：<el-input v-model=\"MRiskManagement.fInspectionPeople\" placeholder=\"请输入内容\" style=\"width:180px;border: 1px solid #DCDFE6;\" :disabled=\"true\"></el-input></span>\n              <div class=\"block\"  style=\"display:inline;margin-left:10px;\">\n                <span class=\"demonstration\" v-html=\"'时&ensp;&ensp;间：'\"/>\n                <el-date-picker style=\"border: 1px solid #DCDFE6;\"\n                  type=\"dates\"\n                  v-model=\"MRiskManagement.fInspectionDate\"\n                  placeholder=\"选择日期\"\n                  format=\"yyyy 年 MM 月 dd 日\"\n                  value-format=\"yyyy-MM-dd HH:mm:ss\"\n                  :disabled=\"true\">\n                </el-date-picker>\n              </div>\n            </div>\n          </div>\n        </el-col>\n      </el-row>\n\n    </div>\n  </div>\n</template>\n\n<script>\n  //获得系统用户\n  import {getUserList } from \"@/api/security/planning\"\n  import {getToken} from \"@/utils/auth\"\n  import {getFileById} from \"@/api/file\"\n  import {listManagement, getManagement, addManagement, updateManagement, delManagement} from '@/api/risk/risk'\n  export default {\n\n    data() {\n      return {\n        // 文件上传\n        upload: {\n          // 设置上传的请求头部\n          headers: {Authorization: \"Bearer \" + getToken()},\n          // 上传的地址\n          url: \"http://localhost/dev-api/file/upload\"\n        },\n        active:1,\n        info: {\n          stockDate:this.getNowTime(),  //日期\n        },\n        dialogVisible:false,\n        //下一处理人 弹窗\n        nextVisible:false,\n        //处理人\n        radio:'',\n        textarea:'',\n        value1: '',\n        value2: '',\n        value3: '',\n        value4: '',\n        input:'',\n        value: '',\n        textarea1:'',\n        textarea2:'',\n        audit: [],\n\n        fileList: [],\n        fileList1: [],\n        fileList2: [],\n        //对象\n        MRiskManagement:{\n          //险情管理ID\n          fId:'',\n          //险情名称\n          fRiskName:'',\n          //险情类别\n          fRiskType:'',\n          //险情编号\n          fRiskNumber:'',\n          //险情发现时间\n          fRiskFindDate:'',\n\n          //险情概述\n          fRiskSummary:'',\n          //记录人\n          fRecordPeople:'',\n          //记录时间\n          fRecordDate:'',\n          //险情信息附件ID\n          fRiskInfoFileId:'',\n\n          //响应情况\n          fResponseSituation:'',\n          //响应人\n          fResponsePeople:'',\n          //响应时间\n          fResponseDate:'',\n          //响应信息附件ID\n          fResponseInfoFileId:'',\n\n          //处置结果\n          fDisposalResults:'',\n          //处置人\n          fDisposalPeople:'',\n          //处置时间\n          fDisposalDate:'',\n          //处置信息附件ID\n          fDisposalInfoFileId:'',\n\n          //检查意见\n          fInspectionOpinions:'',\n          //检查结果\n          fInspectionResults:'',\n          //检查人\n          fInspectionPeople:'',\n          //检查时间\n          fInspectionDate:'',\n\n          //流程实例ID\n          fActId:'',\n          //下一节点审核人\n          fActPersion:'',\n          //流程状态 1已提交启动 2：审核中 3：驳回 4：审核完成\n          fActStatus:'',\n          //流程节点\n          fActNode:'',\n          //创建人\n          fCreateBy:'',\n          //创建时间\n          fCreateTime:'',\n        },\n        //对象\n        MRiskManagementInit:{\n          //险情管理ID\n          fId:'',\n          //险情名称\n          fRiskName:'',\n          //险情类别\n          fRiskType:'',\n          //险情编号\n          fRiskNumber:'',\n          //险情发现时间\n          fRiskFindDate:'',\n\n          //险情概述\n          fRiskSummary:'',\n          //记录人\n          fRecordPeople:'',\n          //记录时间\n          fRecordDate:'',\n          //险情信息附件ID\n          fRiskInfoFileId:'',\n\n          //响应情况\n          fResponseSituation:'',\n          //响应人\n          fResponsePeople:'',\n          //响应时间\n          fResponseDate:'',\n          //响应信息附件ID\n          fResponseInfoFileId:'',\n\n          //处置结果\n          fDisposalResults:'',\n          //处置人\n          fDisposalPeople:'',\n          //处置时间\n          fDisposalDate:'',\n          //处置信息附件ID\n          fDisposalInfoFileId:'',\n\n          //检查意见\n          fInspectionOpinions:'',\n          //检查结果\n          fInspectionResults:'',\n          //检查人\n          fInspectionPeople:'',\n          //检查时间\n          fInspectionDate:'',\n\n          //流程实例ID\n          fActId:'',\n          //下一节点审核人\n          fActPersion:'',\n          //流程状态 1已提交启动 2：审核中 3：驳回 4：审核完成\n          fActStatus:'',\n          //流程节点\n          fActNode:'',\n          //创建人\n          fCreateBy:'',\n          //创建时间\n          fCreateTime:'',\n        },\n      };\n    },\n    created() {\n      this.personnel()\n      this.getDict()\n    },\n    methods: {\n      getDict(){\n      \tthis.getDicts(\"risk_managementr\").then(response => {\n      \t\tthis.audit = response.data;\n          console.log(\"this.audit\",this.audit)\n      \t});\n      },\n\n       beforeAvatarUpload(file){\n            const isLt100M = file.size / 1024 / 1024 < 100;\n               if (!isLt100M) {\n                      this.$message.error('上传文件大小不能超过 100MB!');\n                      return false\n                  }\n            },\n      handlePreview(file) {\n        console.log(file);\r\n\t\t// window.location.href = file.url;\r\n\t\twindow.open(file.url);\n      },\n      handleRemove(file, fileList) {\n        this.GLOBAL.commonFun(this.MRiskManagement.fRiskInfoFileId);\n      },\n      //删除文件的\n      beforeRemove(file, fileList) {\n        return this.$confirm(`确定移除 ${file.name}？`);\n      },\n      //上传数量 限制\n      handleExceed(files, fileList) {\n      this.$message.warning(`当前限制选择 1 个文件，本次选择了 ${files.length} 个文件，共选择了 ${files.length + fileList.length} 个文件`);\n      },\n      //上传成功\n      uploadedSuccess(response, file, fileList) {\n        console.log('上传成功',response)\n        this.MRiskManagement.fRiskInfoFileId = response.data.fId\n        this.msgSuccess(\"上传成功\")\n      },\n\n      //获取处理人集合\n      personnel(){\n        console.log('获取处理人集合')\n        getUserList().then(response => {\n           console.log('获取处理人集合执行成功',response)\n          this.list = response.data\n        })\n      },\n      //保存按钮\n      save() {\n        this.MRiskManagement.fActStatus = 0\n        addManagement(this.MRiskManagement).then(res => {\n          if(res.code == 200) {\n            this.msgSuccess(\"提交成功\")\n            this.MRiskManagement = this.MRiskManagementInit\n            this.fileList = []\n          }\n        })\n      },\n\n      //提交按钮\n      addsubmit() {\n        //是否填写 险情名称\n        if(this.MRiskManagement.fRiskName == null || this.MRiskManagement.fRiskName == '') {\n          this.$message({\n            showClose: true,\n            message: '请填写险情名称',\n            type: 'error'\n          });\n          return false\n        }\n        //是否填写 险情类别\n        if(this.MRiskManagement.fRiskType == null || this.MRiskManagement.fRiskType == '') {\n          this.$message({\n            showClose: true,\n            message: '请选择险情类别',\n            type: 'error'\n          });\n          return false\n        }\n        //是否填写 险情编号\n        if(this.MRiskManagement.fRiskNumber == null || this.MRiskManagement.fRiskNumber == '') {\n          this.$message({\n            showClose: true,\n            message: '请填写险情编号',\n            type: 'error'\n          });\n          return false\n        }\n        //是否填写 险情发现时间\n        if(this.MRiskManagement.fRiskFindDate == null || this.MRiskManagement.fRiskFindDate == '') {\n          this.$message({\n            showClose: true,\n            message: '请选择险情发现时间',\n            type: 'error'\n          });\n          return false\n        }\n        //是否填写 险情概述\n        if(this.MRiskManagement.fRiskSummary == null || this.MRiskManagement.fRiskSummary == '') {\n          this.$message({\n    